{
	"name": "southridge_datalake",
	"properties": {
		"linkedServiceName": {
			"referenceName": "southridge_datalake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"cw_fileName": {
				"type": "String"
			},
			"cw_folder": {
				"type": "String"
			}
		},
		"type": "AzureBlobFSFile",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": true
			},
			"fileName": {
				"value": "@dataset().cw_fileName",
				"type": "Expression"
			},
			"folderPath": "southridge/southridge/@dataset().cw_folder"
		}
	}
}